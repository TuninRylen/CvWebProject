@model CvProject.Models.Entity.TblSosyalmedya
@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Hobi Ekleme Sayfasi</h4>
<br />

<form class="form-group" method="post">
    @Html.Label("Ad")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })

    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })

    @Html.Label("İkon kodu")
    @Html.TextBoxFor(x => x.Ikon, new { @class = "form-control" })

    <br />

    <button class="btn btn-info">Kaydet</button>
</form>

