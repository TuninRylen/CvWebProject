
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim EKleme Sayfasi</h4>
<br />

<form class="form-group" method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı yazınız"/>
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlığı Yazınız"/>
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Deneyim Tarihi"/>
    <br />
    <textarea type="text" name="Aciklama" class="form-control" placeholder="Açıklamayı yazınız" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

























