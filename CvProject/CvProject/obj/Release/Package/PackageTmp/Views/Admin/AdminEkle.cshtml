@model CvProject.Models.Entity.TblAdmin

@{
    ViewBag.Title = "AdminEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Admin Ekleme Sayfasi</h4>
<br />

<form class="form-group" method="post">

    @Html.Label("Kullanıcı Adı")
    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })

    @Html.Label("Şifre")
    @Html.TextBoxFor(x => x.Sifre, new { @class = "form-control" })

    <br />

    <button class="btn btn-info">Kaydet</button>
</form>

